{
  "name": "@marketing-engine/kpis",
  "version": "1.0.0",
  "description": "Comprehensive KPI calculation engine for marketing analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:migrate": "node dist/migrations/migrate.js",
    "db:seed": "node dist/scripts/seed.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "benchmark": "ts-node scripts/benchmark.ts"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^4.6.12",
    "ioredis": "^5.3.2",
    "decimal.js": "^10.4.3",
    "date-fns": "^3.0.6",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "prom-client": "^15.1.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "bull": "^4.12.0",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.202"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}