{
  "name": "@warehouse-network/claude-agent-tracker",
  "version": "1.0.0",
  "description": "Enterprise agent tracking and change management platform with real-time monitoring, PostgreSQL persistence, and JWT authentication",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "claude-agent-tracker": "bin/claude-agent-tracker.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "build:watch": "tsc --watch",
    "copy-assets": "cp -r src/templates dist/ 2>/dev/null || true",
    "dev": "npm run build:watch",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{ts,js} --fix",
    "typecheck": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=moderate",
    "docker:build": "docker build -t claude-agent-tracker .",
    "docker:run": "docker run -p 3000:3000 --env-file .env claude-agent-tracker",
    "db:migrate": "node dist/cli/migrate.js",
    "db:seed": "node dist/cli/seed.js",
    "mcp": "node dist/mcp-server.js",
    "agent": "node dist/agent-runner.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "claude",
    "ai-agent",
    "mcp-server",
    "tracking",
    "reporting",
    "change-management",
    "automation",
    "project-management"
  ],
  "author": "Claude Agent Tracker Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/resources": "2.2.0",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "graceful-shutdown": "^0.1.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "ora": "^8.0.1",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "redis": "^4.6.10",
    "semver": "^7.5.4",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@opentelemetry/auto-instrumentations-node": "0.40.3",
    "@opentelemetry/exporter-trace-otlp-http": "0.208.0",
    "@opentelemetry/sdk-node": "0.46.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "^20.8.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2",
    "winston-daily-rotate-file": "5.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "docs/**/*",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warehouse-network/platform.git",
    "directory": "packages/claude-agent-tracker"
  },
  "bugs": {
    "url": "https://github.com/warehouse-network/platform/issues"
  },
  "homepage": "https://github.com/warehouse-network/platform/tree/main/packages/claude-agent-tracker#readme",
  "publishConfig": {
    "access": "public"
  }
}
