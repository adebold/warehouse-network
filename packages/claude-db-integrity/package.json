{
  "name": "claude-db-integrity",
  "version": "1.0.0",
  "description": "Comprehensive database integrity system with Claude Flow memory integration for production applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "claude-db-integrity": "./bin/claude-db-integrity"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run copy-assets",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "copy-assets": "cp -r templates dist/ && cp -r examples dist/ && cp -r docs dist/",
    "dev": "npm run build:watch",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build",
    "postinstall": "node bin/post-install.js || echo 'Postinstall completed'",
    "publish:dry": "npm publish --dry-run",
    "publish:beta": "npm publish --tag beta",
    "publish:latest": "npm publish --tag latest",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "docs:generate": "typedoc --out docs/api src/index.ts",
    "examples:test": "node scripts/test-examples.js",
    "quick-setup": "./scripts/quick-setup.sh",
    "migrate-existing": "./scripts/migrate-existing.sh",
    "docker-setup": "./scripts/docker-setup.sh"
  },
  "keywords": [
    "database",
    "integrity",
    "validation",
    "claude-flow",
    "memory",
    "schema",
    "migration",
    "prisma",
    "typeorm",
    "nextjs",
    "express",
    "nestjs",
    "monitoring",
    "drift-detection"
  ],
  "author": "Claude DB Integrity Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/warehouse-network/claude-db-integrity.git"
  },
  "bugs": {
    "url": "https://github.com/warehouse-network/claude-db-integrity/issues"
  },
  "homepage": "https://github.com/warehouse-network/claude-db-integrity#readme",
  "files": [
    "dist/**/*",
    "bin/**/*",
    "scripts/**/*",
    "README.md",
    "LICENSE",
    "PUBLISHING.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./express": {
      "import": "./dist/integrations/express.js",
      "require": "./dist/integrations/express.js",
      "types": "./dist/integrations/express.d.ts"
    },
    "./nextjs": {
      "import": "./dist/integrations/nextjs.js",
      "require": "./dist/integrations/nextjs.js", 
      "types": "./dist/integrations/nextjs.d.ts"
    },
    "./nestjs": {
      "import": "./dist/integrations/nestjs.js",
      "require": "./dist/integrations/nestjs.js",
      "types": "./dist/integrations/nestjs.d.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "joi": "^17.11.0",
    "ajv": "^8.12.0",
    "winston": "^3.11.0",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "yaml": "^2.3.4",
    "glob": "^10.3.10",
    "puppeteer": "^21.6.1",
    "axe-core": "^4.8.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.202",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.3",
    "eslint": "^8.55.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.4",
    "@jest/types": "^29.6.3",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "prisma": "^5.0.0",
    "typeorm": "^0.3.0",
    "nextjs": "^14.0.0"
  },
  "peerDependenciesMeta": {
    "prisma": {
      "optional": true
    },
    "typeorm": {
      "optional": true
    },
    "nextjs": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}