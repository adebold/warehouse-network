# Database
DATABASE_URL="postgresql://warehouse:warehouse123@localhost:5432/warehouse_network"
DB_USER=warehouse
DB_PASSWORD=warehouse123
DB_NAME=warehouse_network

# Application
NODE_ENV=development
PORT=3000

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=development-secret-key-change-in-production

# Redis
REDIS_URL=redis://localhost:6379

# Stripe (development keys - replace with your test keys)
STRIPE_SECRET_KEY=sk_test_placeholder
STRIPE_WEBHOOK_SECRET=whsec_placeholder

# Cron Jobs
CRON_SECRET_ACCRUAL=dev-cron-secret-accrual
CRON_SECRET_PAYOUTS=dev-cron-secret-payouts
CRON_SECRET_SCORES=dev-cron-secret-scores

# File Uploads
UPLOAD_MAX_SIZE=10485760
UPLOAD_DIR=/app/uploads

# Security
JWT_SECRET=development-jwt-secret
BCRYPT_SALT_ROUNDS=12

# Monitoring
LOG_LEVEL=debug
ENABLE_METRICS=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100