# Staging Environment Variables
NODE_ENV=staging
DATABASE_URL=postgresql://user:pass@db:5432/warehouse_staging
NEXT_PUBLIC_API_URL=https://staging.yourdomain.com/api

# Database
DB_USER=staging_user
DB_PASSWORD=staging_password_here
DB_NAME=warehouse_staging

# Redis
REDIS_URL=redis://redis:6379

# JWT
JWT_SECRET=your-staging-jwt-secret-here

# External APIs
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Monitoring
SENTRY_DSN=your-sentry-dsn